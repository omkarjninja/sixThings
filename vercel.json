{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "^/_expo/static/(.*)$", "dest": "/_expo/static/$1" },

    { "src": "^/(favicon\\.ico|manifest\\.json|metadata\\.json)$", "dest": "/$1" },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
